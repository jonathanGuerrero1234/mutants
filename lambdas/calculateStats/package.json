{
  "name": "calculate-stats",
  "version": "1.0.0",
  "description": "calculate stats",
  "main": "src/index.js",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "tsc -p tsconfig.json && npm run copy-pck-file && npm run zip",
    "copy-pck-file": "cp package.json dist/ && cd dist && npm i --production ",
    "zip": "cd dist/ && bestzip  ../calculate_stats.zip *",
    "postbuild": "mv calculate_stats.zip dist/",
    "lint": "eslint '*/**/*.ts' --quiet",
    "lint:fix": "eslint '*/**/*.ts' --quiet --fix",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "coverage": "nyc -r lcov -r text -e .ts -x \"test/**/*.spec.ts\" npm run test"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Jonathan Guerrero",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "http-status-codes": "^2.1.4",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.14",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "bestzip": "^2.1.7",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jasmine": "^3.6.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "aws-sdk": "^2.834.0",
    "sonar-scanner": "^3.1.0"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/inversify.config.ts"
    ]
  }
}
